      "name": "input_file",
      "label": "Variants",
      "class": "file",
      "optional": false,
      "patterns": [
        "*.vcf",
        "*.vcf.gz"
      ],
      "help": "Input file containing variants"
    },
    {
      "name": "package",
      "label": "Package",
      "class": "string",
      "optional": true,
      "help": "Analysis package to use",
      "choices":[
        "default",
        "drug_interaction",
        "hereditary_cancer",
        "pathogenic",
        "rare_coding",
        "splicing"
      ]
    },
    {
      "name": "genome",
      "label": "Genome",
      "class": "string",
      "optional": false,
      "choices":[
        "hg38",
        "hg19",
        "hg18"
