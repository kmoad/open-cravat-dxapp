      ],
      "help": ""
    },
    {
      "name": "log",
      "label": "Log",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "err",
      "label": "Error List",
      "class": "file",
      "patterns": [
        "*"
      ],
